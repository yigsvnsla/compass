{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": ".",
  "cli": {
    "analytics": false,
    "packageManager": "pnpm",
    "schematicCollections": [
      "@ngrx/schematics"
    ]
  },
  "projects": {
    "web-compass": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "standalone": true
        },
        "@schematics/angular:directive": {
          "standalone": true
        },
        "@schematics/angular:pipe": {
          "standalone": true
        }
      },
      "root": "projects/web-compass",
      "sourceRoot": "projects/web-compass/src",
      "prefix": "web",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/web-compass/browser",
            "index": "projects/web-compass/src/index.html",
            "main": "projects/web-compass/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/web-compass/tsconfig.app.json",
            "assets": [
              "projects/web-compass/src/favicon.ico",
              "projects/web-compass/src/assets",
              "projects/web-compass/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/web-compass/src/styles.css"
            ],
            "scripts": [],
            "serviceWorker": true,
            "ngswConfigPath": "projects/web-compass/ngsw-config.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "web-compass:build:production"
            },
            "development": {
              "browserTarget": "web-compass:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-compass:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "web-compass/tsconfig.spec.json",
            "assets": [
              "projects/web-compass/src/favicon.ico",
              "projects/web-compass/src/assets",
              "projects/web-compass/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/web-compass/src/styles.css"
            ],
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/web-compass/server",
            "main": "projects/web-compass/server.ts",
            "tsConfig": "projects/web-compass/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false,
              "vendorChunk": true
            }
          },
          "defaultConfiguration": "production"
        },
        "app-shell": {
          "builder": "@angular-devkit/build-angular:app-shell",
          "options": {
            "route": "shell"
          },
          "configurations": {
            "production": {
              "browserTarget": "web-compass:build:production",
              "serverTarget": "web-compass:server:production"
            },
            "development": {
              "browserTarget": "web-compass:build:development",
              "serverTarget": "web-compass:server:development"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "web-compass:build:development",
              "serverTarget": "web-compass:server:development"
            },
            "production": {
              "browserTarget": "web-compass:build:production",
              "serverTarget": "web-compass:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {
              "browserTarget": "web-compass:build:production",
              "serverTarget": "web-compass:server:production"
            },
            "development": {
              "browserTarget": "web-compass:build:development",
              "serverTarget": "web-compass:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "app-compass": {
      "cli": {
        "schematicCollections": [
          "@ionic/angular-toolkit"
        ]
      },
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": true
        },
        "@schematics/angular:directive": {
          "standalone": true
        },
        "@schematics/angular:pipe": {
          "standalone": true
        },
        "@ionic/angular-toolkit:component": {
          "styleext": "scss"
        },
        "@ionic/angular-toolkit:page": {
          "styleext": "scss"
        }
      },
      "root": "projects/app-compass",
      "sourceRoot": "projects/app-compass/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app-compass",
            "index": "projects/app-compass/src/index.html",
            "main": "projects/app-compass/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/app-compass/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/app-compass/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              "projects/app-compass/src/global.scss",
              "projects/app-compass/src/theme/variables.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "app-compass:build:production"
            },
            "development": {
              "browserTarget": "app-compass:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-compass:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "app-compass/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/compass/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              "projects/app-compass/src/global.scss",
              "projects/app-compass/src/theme/variables.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}